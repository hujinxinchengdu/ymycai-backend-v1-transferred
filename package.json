{
  "name": "stockaid",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon src/bin/www.ts",
    "prettify-all": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,scss,json,yaml}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,css,scss,json,yaml}": [
      "prettier --write"
    ]
  }
}
