{
  "name": "stockaid-backend",
  "version": "1.0.0",
  "description": "## About",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "dev": "nodemon --watch './**/*.ts' --exec ts-node src/index.ts",
    "prettify-all": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,scss,json,yaml}\"",
    "prepare": "husky install",
    "start:local_dev": "cross-env NODE_ENV=local_dev nodemon --watch './**/*.ts' --exec ts-node src/index.ts",
    "start:dev": "NODE_ENV=dev nodemon --watch './**/*.ts' --exec ts-node src/index.ts",
    "start:cloud_dev": "cross-env NODE_ENV=cloud_dev nodemon --watch './**/*.ts' --exec ts-node src/index.ts",
    "start:local_product": "NODE_ENV=local_product nodemon --watch './**/*.ts' --exec ts-node src/index.ts",
    "start:product": "NODE_ENV=product nodemon --watch './**/*.ts' --exec ts-node src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "~16.3.1",
    "express": "~4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.0.0",
    "morgan": "~1.10.0",
    "pg": "~8.11.1",
    "typeorm": "~0.3.17"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "~4.17.17",
    "@types/node": "~20.3.1",
    "husky": "~8.0.3",
    "lint-staged": "~13.2.2",
    "nodemon": "~2.0.22",
    "prettier": "~2.8.8",
    "rimraf": "~5.0.1",
    "ts-node": "~10.9.1",
    "typescript": "~5.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,css,scss,json,yaml}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Skyline-Dev-Labs/stockaid-backend.git"
  },
  "bugs": {
    "url": "https://github.com/Skyline-Dev-Labs/stockaid-backend/issues"
  },
  "homepage": "https://github.com/Skyline-Dev-Labs/stockaid-backend#readme"
}
